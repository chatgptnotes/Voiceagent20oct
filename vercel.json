{
  "name": "voiceagent20oct",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_AGENT_ID": "@react_app_agent_id",
    "REACT_APP_ELEVENLABS_API_KEY": "@react_app_elevenlabs_api_key"
  }
}